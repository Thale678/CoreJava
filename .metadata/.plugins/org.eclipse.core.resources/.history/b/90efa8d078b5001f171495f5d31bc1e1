package corejava.basic;

public class ifelse {
	
	
	public static void main(String[] args) {
	int x = 20;
	int y = 30;
	
	if(x < y)  // x is greater than 10 and less than or equal to 20;
	{
		System.out.println(x);
		System.out.println(x+" is gretaer than 10");
	}
	else 
		System.out.println("X is lesser than 10");
	
  
		
	}

}
