package corejava.basic;

public class arithmaticoperators {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int num1 = 8;
		int num2 = 3;
		
		int result = num1 + num2;
		
		System.out.println(result);
		result = num1 - num2;
		System.out.println(result);
		result = num1 * num2;
		System.out.println(result);
		result = num1/num2;
		System.out.println(result);
		result = num1%num2;
		System.out.println(result);
		
		int num3 =13;
		int num4 = 4;
		
		 result = num3 + num4;
		System.out.println(result);
		result = num1 - num2;
		System.out.println(result);
		result = num1 * num2;
		System.out.println(result);
		result = num1/num2;
		System.out.println(result);
		result = num1%num2;
		System.out.println(result);
	}

}
